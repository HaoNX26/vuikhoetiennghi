    <!-- site -->
    <link rel="stylesheet" href="/css/site_admin.css" asp-append-version="true">
    <!-- ./wrapper -->
    <!-- jQuery 3 -->
    <script src="/bower_components/jquery/dist/jquery-2.1.1.min.js"></script>
