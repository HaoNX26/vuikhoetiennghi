</div>

<script type="text/javascript">
    if ('@ViewBag.Message') alert('@ViewBag.Message')
    //
    $(document).ready(function () {
        //
        $('.date-picker').datetimepicker({
            locale: 'en', keepOpen: false, showClose: true,
            format: 'DD-MM-YYYY'
        });
        $('.datepicker').datepicker();
        //
        $('.select2').select2({
            width: "100%"
        });

        var fid = $('#fid').val();
         var CustomerStatus = $('#CUSTOMER_STATUS').val();
        if (CustomerStatus === '11' && fid === '3') {
            $("#NOTE_CUS_REJECT").prop('disabled', false);

        } else {
            $("#NOTE_CUS_REJECT").prop('disabled', true);
        }
    });

      function AddEditDialog(itemId) {
        var url = "/Business/ViewFileCustomerHistory?id=" + itemId;
            $('#title').html("Lịch sử nộp hồ sơ");
        //
        showLoading();
        $("#edit-body-content").load(url, function (e, responseText) {
            if (responseText === "error") { location.reload() }
            $("#modal-default").modal({ backdrop: 'static', keyboard: false })
            //$(this).find("input:not(:submit,:reset,:button,:hidden),select,textarea")[0].focus()
            hideLoading();
        });
    }

    function checkNoteReject() {
        var CustomerStatus = $('#CUSTOMER_STATUS').val();
        if (CustomerStatus === '11') {
            $("#NOTE_CUS_REJECT").prop('disabled', false);

        } else {
            $("#NOTE_CUS_REJECT").prop('disabled', true);
        }
    }

    function coppyReason() {
        var NOTE_OF_AGENCY = $('#NOTE_OF_AGENCY').val();
        $('#NOTE_CUS_REJECT').val(NOTE_OF_AGENCY);
    }
</script>