                                <textarea asp-for="NOTE_CUS_REJECT" rows="3" disabled class="form-control"></textarea>
                            </div>
                            <button type="button" onclick="coppyReason()" class="btn btn-primary">Sao chép lý do</button>
                            <div class="form-group">
                                <label asp-for="NOTE_OF_ADMIN">
                                    Ghi chú của admin
                                </label>
                                <textarea asp-for="NOTE_OF_ADMIN" rows="3" class="form-control"></textarea>
                            </div>
                        </div>
                    </div>
                }

                <div class="form-group">
                    <button type="submit" class="btn btn-primary" asp-action="SaveFileCustomerWin" asp-controller="Business">Ghi lại</button>
                    <button asp-action="BackToViewFileCustomerWin" class="btn btn-secondary" asp-controller="Business">Đóng</button>
                </div>
            </div>
        </form>

    </div>

</div>

